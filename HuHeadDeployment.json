{
  "kind": "Deployment",
  "apiVersion": "apps/v1",
  "metadata": {
    "name": "hyperupheaddeployment",
    "generateName": "hyperupheadinstance",
    "namespace": "default"
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "app": "hyperupheadinstance"
      }
    },
    "imagePullSecrets": [ { "name": "regcred" } ],
    "template": {
      "metadata": {
        "labels": {
          "app": "hyperupheadinstance"
        },
        "name": "hyperuphead",
        "generateName": "hyperuphead"
      },
      "spec": {
        "containers": [
          {
            "name": "huhead",
            "image": "192.168.1.11:5000/dockerarvi/hyperupheaditgrad",
            "ports": [
              {
                "containerPort": 5008,
                "protocol": "TCP"
              }
            ]
          }
        ]
      }
    }
  }
}
